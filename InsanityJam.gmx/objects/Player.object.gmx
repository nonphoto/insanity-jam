          <argument>
            <kind>1</kind>
            <string>// Move object
if (xx &lt; x) {
    x = max(x - move_speed, xx)
}
if (xx &gt; x) {
    x = min(x + move_speed, xx)
}
if (yy &lt; y) {
    y = max(y - move_speed, yy)
}
if (yy &gt; y) {
    y = min(y + move_speed, yy)
}

// Check if completed movement
if (y == yy &amp;&amp; x == xx) {
    moving = false
}
// Animation
if(moving == true){
        image_speed = .5
}
else{
        image_speed = .1
}

// Check keyboard
if (keyboard_check(vk_left) &amp;&amp; !moving &amp;&amp; editing &lt;= 0 &amp;&amp; place_free(x - tile_size, y)) {
    moving = true
    xx -= tile_size
    sprite_index = spr_left
}
if (keyboard_check(vk_right) &amp;&amp; !moving &amp;&amp; editing &lt;= 0 &amp;&amp; place_free(x + tile_size, y)) {
    moving = true
    xx += tile_size
    sprite_index = spr_right
}
if (keyboard_check(vk_up) &amp;&amp; !moving &amp;&amp; editing &lt;= 0 &amp;&amp; place_free(x, y - tile_size)) {
    moving = true
    yy -= tile_size
}
if (keyboard_check(vk_down) &amp;&amp; !moving &amp;&amp; editing &lt;= 0 &amp;&amp; place_free(x, y + tile_size)) {
    moving = true
    yy += tile_size
}
if (pressed_enter &amp;&amp; !moving) {
    var other_drone = instance_place(x, y, Drone)
    var other_code = instance_place(x, y, Code)
    if (editing &gt; 0) {
        // End editing
        editing = noone
        event_perform(ev_user1, 0)
        player_end_edit()
    }
    else if (other_drone &gt; 0) {
        // Start editing
        editing = other_drone.id
        var l = ds_list_size(editing.logic);
        if (ds_list_find_value(editing.logic, l - 1) != 0 || ds_list_find_value(editing.action, l - 1) != 0) {
            ds_list_insert(editing.logic, l, 0);
            ds_list_insert(editing.action, l, 0);
        }
        event_perform(ev_user0, 0);
        player_start_edit()
    }
    else if (held &gt; 0) {
        // Drop item
        held.x = x
        held.y = y
        held.visible = true
        held = noone
    }
    else if (other_code &gt; 0 &amp;&amp; other_code.visible == true) {
        // Pick up item
        held = other_code
        held.visible = false
    }
    
    pressed_enter = false
}
</string>
